{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:remix && npm run build:fastly",
    "build:remix": "remix build",
    "prebuild:fastly": "compute-js-static-publish --build-static --suppress-framework-warnings",
    "build:fastly": "js-compute-runtime ./src/index.js ./bin/main.wasm",
    "deploy": "fastly compute publish",
    "dev:remix": "remix watch",
    "dev:fastly": "fastly compute serve --watch",
    "dev": "run-p \"dev:*\"",
    "start": "fastly compute serve"
  },
  "dependencies": {
    "@fastly/remix-server-runtime": "^3.0.0-beta.0",
    "@fastly/remix-server-adapter": "^3.0.0-beta.0",
    "@remix-run/react": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fastly/compute-js-static-publish": "^5.1.1",
    "@fastly/js-compute": "^2.5.0",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
